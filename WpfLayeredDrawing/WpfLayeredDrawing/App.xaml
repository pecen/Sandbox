    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:WpfLayeredDrawing"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/WpfLayeredDrawing;component/Brushes.xaml" />
                <ResourceDictionary Source="/WpfLayeredDrawing;component/Geometries.xaml" />
                <ResourceDictionary Source="/WpfLayeredDrawing;component/Fonts.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
